{
  "app": "npx ts-node --prefer-ts-exts bin/cdk.ts",
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "aws-cdk:enableDiffNoFail": "true",
    "@aws-cdk/core:stackRelativeExports": "true",
    "@aws-cdk/aws-ecr-assets:dockerIgnoreSupport": true,
    "@aws-cdk/aws-secretsmanager:parseOwnedSecretName": true,
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "projectName": "lambda_auth_blueprints",
    "description": "Infrastructure for lambda_auth authorizer",
    "gitTokenPath": "/all/github/ndlib-git",
    "gitOwner": "ndlib",
    "serviceRepository": "lambda_auth",
    "serviceBranch": "master",
    "blueprintsRepository": "lambda_auth_blueprints",
    "blueprintsBranch": "main",
    "sentryTokenPath": "/all/sentry/ci-token",
    "sentryOrg": "university-of-notre-dame-ts",
    "sentryProject": "lambda-auth",
    "emailReceivers": "wse-notifications-group@nd.edu"
  }
}
